<?xml version="1.0" encoding="utf-8" ?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <appSettings>
    <add xdt:Transform="InsertIfMissing" xdt:Locator="Match(key)" key="ClientValidationEnabled" value="true" />
    <add xdt:Transform="InsertIfMissing" xdt:Locator="Match(key)" key="UnobtrusiveJavaScriptEnabled" value="true" />
    <add xdt:Transform="InsertIfMissing" xdt:Locator="Match(key)" key="localenv:define" value="Development" />
    <add xdt:Transform="SetAttributes" xdt:Locator="Match(key)" key="localenv:define" value="Development" />
  </appSettings>
</configuration>